{{ header }}
<div id="blog-content" class="container">
    <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
            <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
    </ul>
    <div class="row">
        <div id="content" class="col-12">
            <h1>{{ blog_header }}</h1>
            {{ blog_content }}
        </div>
    </div>
</div>
{{ footer }}